{
  "name": "agnos-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "production": "cross-env TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/src",
    "local": "ts-node -r tsconfig-paths/register src",
    "start": "nodemon --ignore __test__/ -e ts,json --exec npm run local",
    "prisma-test": "dotenv -e .env.test prisma migrate deploy",
    "prisma-staging": "dotenv -e .env.staging prisma migrate deploy",
    "prisma-production": "dotenv -e .env.production prisma migrate deploy",
    "prisma-reset": "dotenv -e .env.staging prisma migrate reset",
    "seed": "prisma db seed",
    "test": "jest --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@prisma/client": "^6.4.1",
    "@simplewebauthn/server": "^13.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.5.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.11",
    "objectify-response": "^1.0.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --transpile-only prisma/seed.ts"
  }
}
